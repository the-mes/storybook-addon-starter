{
  "name": "storybook-addon-starter",
  "version": "0.0.0",
  "description": "Template for Storybook Addon",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "eslint 'src/**/*.{tsx,ts}' --fix",
    "//prettier": "prettier --write 'src/**/*' 'test/**/*' 'README.md' '/docs/**/*.md'",
    "prettier": "prettier --write 'src/**/*.{html,scss,css,tsx,ts}'",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/the-mes/storybook-addon-starter.git"
  },
  "keywords": [
    "react",
    "stories",
    "template",
    "typescript",
    "extension",
    "addon",
    "starter",
    "storybook"
  ],
  "files": [
    "dist/**/*",
    "register.js"
  ],
  "homepage": "",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addons": "^6.3.12",
    "@storybook/api": "^6.3.12",
    "@storybook/components": "^6.3.12",
    "@storybook/react": "^6.5.13",
    "@storybook/storybook-deployer": "^2.8.10",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "npm run prettier"
    ]
  }
}
